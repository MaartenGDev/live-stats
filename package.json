{
  "name": "live-stats",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel watch:*",
    "deploy": "npm-run-all --parallel prod:*",
    "watch:sass": "node-sass src/assets/sass/app.sass public/css/app.css --watch",
    "watch:react": "webpack --watch",
    "prod:sass": "node-sass src/assets/sass/app.sass public/css/app.css --output-style compressed",
    "prod:webpack": "webpack -p",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hbs": "~4.0.1",
    "morgan": "~1.7.0",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "serve-favicon": "~2.3.0",
    "webpack": "^2.2.1"
  }
}
