{
  "name": "live-stats",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel watch:*",
    "deploy": "npm-run-all --parallel prod:*",
    "watch:sass": "node-sass src/assets/sass/app.sass public/css/app.css --watch",
    "prod:sass": "node-sass src/assets/sass/app.sass public/css/app.css --output-style compressed",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hbs": "~4.0.1",
    "morgan": "~1.7.0",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "serve-favicon": "~2.3.0"
  }
}
